package com.vanguard.corp.ics.dao.staging;

import java.util.Date;

import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

import com.vanguard.corp.ics.dao.FileDAO;
import com.vanguard.corp.ics.dao.PortfolioDAO;
import com.vanguard.corp.ics.dao.target.DatasourceFileDAO;

@Entity
@Table(name = "SDLSADM.TSCD_VALUTN_FILE")
public class ValuationsStagingDAO implements PortfolioDAO<Long>, FileDAO<Long> {

	private static final long serialVersionUID = -1540114032167233651L;

	@Id
	@GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "SDLSADM.SEQ1SCDVALUTNFILEID")
	@SequenceGenerator(name = "SDLSADM.SEQ1SCDVALUTNFILEID", sequenceName = "SDLSADM.SEQ1SCDVALUTNFILEID", initialValue = 1, allocationSize = 100)
	@Column(name = "SCD_VALUTN_FILE_ID", unique = true, nullable = false)
	private Long id;

	@ManyToOne(cascade = { CascadeType.MERGE })
	@JoinColumn(name = "DATA_SRC_FILE_ID")
	private DatasourceFileDAO datasourceFileDAO;

	@Column(name = "RUN_DTTM", nullable = false)
	private Date runDateTime;

	@Column(name = "VAL_DT", nullable = false)
	private Date valueDate;

	@Column(name = "PORT_CD", nullable = false)
	private String portfolioCode;

	@Column(name = "EQTY_VAL")
	private Double equityValue;

	@Column(name = "FUT_VAL")
	private Double futuresValue;

	@Column(name = "FXD_INT_VAL")
	private Double fixedInterestValue;

	@Column(name = "FFX_VAL")
	private Double forwardForiegnExchangeValue;

	@Column(name = "LQDITY_VAL")
	private Double liquidityValue;

	@Column(name = "DERIVATIVE_VAL")
	private Double derivitivesValue;

	@Column(name = "TOT_VAL")
	private Double totalValue;

	@Column(name = "CREATD_BY_USER_ID", nullable = true, insertable = false)
	private String createdUser;

	@Column(name = "CREATD_DT", nullable = true, insertable = false)
	private Date createdDate;

	@Column(name = "LST_UPDTD_USER_ID", nullable = true, insertable = false)
	private String updatedUser;

	@Column(name = "LST_UPDTD_TS", nullable = true, insertable = false)
	private Date updatedDate;

	@Override
	public Long getId() {
		return id;
	}

	@Override
	public void setId(Long id) {
		this.id = id;
	}

	public Date getRunDateTime() {
		return runDateTime;
	}

	public void setRunDateTime(Date runDateTime) {
		this.runDateTime = runDateTime;
	}

	public Date getValueDate() {
		return valueDate;
	}

	public void setValueDate(Date valueDate) {
		this.valueDate = valueDate;
	}

	@Override
	public String getPortfolioCode() {
		return portfolioCode;
	}

	@Override
	public void setPortfolioCode(String portfolioCode) {
		this.portfolioCode = portfolioCode;
	}

	@Override
	public DatasourceFileDAO getDatasourceFileDAO() {
		return datasourceFileDAO;
	}

	@Override
	public void setDatasourceFileDAO(DatasourceFileDAO datasourceFileDAO) {
		this.datasourceFileDAO = datasourceFileDAO;
	}

	public Double getEquityValue() {
		return equityValue;
	}

	public void setEquityValue(Double equityValue) {
		this.equityValue = equityValue;
	}

	public Double getFuturesValue() {
		return futuresValue;
	}

	public void setFuturesValue(Double futureValue) {
		this.futuresValue = futureValue;
	}

	public Double getFixedInterestValue() {
		return fixedInterestValue;
	}

	public void setFixedInterestValue(Double fixedInterestValue) {
		this.fixedInterestValue = fixedInterestValue;
	}

	public Double getForwardForiegnExchangeValue() {
		return forwardForiegnExchangeValue;
	}

	public void setForwardForiegnExchangeValue(Double forwardForiegnExchangeValue) {
		this.forwardForiegnExchangeValue = forwardForiegnExchangeValue;
	}

	public Double getLiquidityValue() {
		return liquidityValue;
	}

	public void setLiquidityValue(Double liquidityValue) {
		this.liquidityValue = liquidityValue;
	}

	public Double getDerivitivesValue() {
		return derivitivesValue;
	}

	public void setDerivitivesValue(Double derivitiveValue) {
		this.derivitivesValue = derivitiveValue;
	}

	public Double getTotalValue() {
		return totalValue;
	}

	public void setTotalValue(Double totalValue) {
		this.totalValue = totalValue;
	}

	public String getCreatedUser() {
		return createdUser;
	}

	public void setCreatedUser(String createdUser) {
		this.createdUser = createdUser;
	}

	public Date getCreatedDate() {
		return createdDate;
	}

	public void setCreatedDate(Date createdDate) {
		this.createdDate = createdDate;
	}

	public String getUpdatedUser() {
		return updatedUser;
	}

	public void setUpdatedUser(String updatedUser) {
		this.updatedUser = updatedUser;
	}

	public Date getUpdatedDate() {
		return updatedDate;
	}

	public void setUpdatedDate(Date updatedDate) {
		this.updatedDate = updatedDate;
	}

	@Override
	public String toString() {
		return "ValuationsStagingDAO [runDateTime=" + runDateTime + ", valueDate=" + valueDate + ", portfolioCode="
				+ portfolioCode + ", equityValue=" + equityValue + ", totalValue=" + totalValue + "]";
	}

}

