com.vanguard.corp.ics.portal.helper
	TokenHelper.class

/*
 ****************************************************************************
 *
 * Copyright (c)2017 The Vanguard Group of Investment Companies (VGI)
 * All rights reserved.
 *
 * This source code is CONFIDENTIAL and PROPRIETARY to VGI. Unauthorized
 * distribution, adaptation, or use may be subject to civil and criminal
 * penalties.
 *
 ****************************************************************************
 Module Description:

 $HeadURL: http://prdsvnrepo:8080/svn/tip/tip/via/PSI/Java/SMA/Shared/PortalService/trunk/src/main/java/com/vanguard/corp/ics/portal/helper/TokenHelper.java $
 $LastChangedRevision: 603652 $
 $Author: USTB $
 $LastChangedDate: 2017-10-16 01:56:58 -0400 (Mon, 16 Oct 2017) $
*/
package com.vanguard.corp.ics.portal.helper;

import javax.ws.rs.client.Client;
import javax.ws.rs.client.ClientBuilder;
import javax.ws.rs.client.Entity;
import javax.ws.rs.client.WebTarget;
import javax.ws.rs.core.Cookie;
import javax.ws.rs.core.Form;
import javax.ws.rs.core.MediaType;
import javax.ws.rs.core.Response;

import org.json.JSONArray;
import org.json.JSONObject;

import com.vanguard.corp.ics.portal.config.PortalSDKConfiguration;
import com.vanguard.services.logging.Log;

public class TokenHelper {

	private TokenHelper() {
	}

	public static Cookie getVGSessionCookie(PortalSDKConfiguration config) {
		return new Cookie("VGSESSION", getToken(config));
	}

	private static String getToken(PortalSDKConfiguration config) {

		try {
			final Client client = ClientBuilder.newClient();
			final WebTarget target = client.target(config.getTokenURL());

			final Form form = new Form();
			form.param("SPX_TOKEN_TYPE", "AD");
			form.param("SPX_TOKEN_USER", config.getPortalUsername());
			form.param("SPX_TOKEN_PASSWORD", config.getPortalPassword());

			final Response response = target.request(MediaType.APPLICATION_JSON_TYPE)
					.post(Entity.entity(form, MediaType.APPLICATION_FORM_URLENCODED_TYPE));

			final String jsonstring = response.readEntity(String.class);

			final JSONObject obj = new JSONObject(jsonstring);
			final JSONArray tokenSet = obj.getJSONArray("TokenSet");
			final JSONObject token = (JSONObject) tokenSet.get(0);
			final String tokenValue = (String) token.get("TokenValue");

			return tokenValue;

		} catch (final Exception e) {
			Log.error(TokenHelper.class.getName(), "Exception while getting auth token:" + e.getMessage(), 1);
		}
		return "";
	}

}

com.vanguard.corp.ics.portal.impl
	PortalServiceImpl.class
/*
 ****************************************************************************
 *
 * Copyright (c)2017 The Vanguard Group of Investment Companies (VGI)
 * All rights reserved.
 *
 * This source code is CONFIDENTIAL and PROPRIETARY to VGI. Unauthorized
 * distribution, adaptation, or use may be subject to civil and criminal
 * penalties.
 *
 ****************************************************************************
 Module Description:

 $HeadURL: http://prdsvnrepo:8080/svn/tip/tip/via/PSI/Java/SMA/Shared/PortalService/trunk/src/main/java/com/vanguard/corp/ics/portal/impl/PortalServiceImpl.java $
 $LastChangedRevision: 603267 $
 $Author: uoex $
 $LastChangedDate: 2017-10-13 02:33:28 -0400 (Fri, 13 Oct 2017) $
*/
package com.vanguard.corp.ics.portal.impl;

import java.util.List;
import java.util.Optional;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

import com.vanguard.corp.ics.portal.PortalService;
import com.vanguard.corp.ics.portal.domain.Currency;
import com.vanguard.corp.ics.portal.domain.Product;
import com.vanguard.corp.ics.portal.repository.PortalCurrencyRepository;
import com.vanguard.corp.ics.portal.repository.PortalProductRepository;

@Component
public class PortalServiceImpl implements PortalService {

	@Autowired
	private PortalProductRepository productRepository;

	@Autowired
	private PortalCurrencyRepository currencyRepository;

	@Override
	public List<Product> getProducts() {
		return productRepository.getEntities();
	}

	@Override
	public Optional<Product> getProduct(String productCode) {
		return productRepository.getEntity(productCode);
	}

	@Override
	public Optional<Product> getProductByRelatedPortfolio(String relatedPortfolioCode, Long applicationId,
			Long portfolioTypeId) {
		return getProducts().stream()
				.filter(product -> product.getRelatedPortfolios() != null)
				.filter(product -> product.getRelatedPortfolios().stream()
						.filter(rp -> applicationId.equals(rp.getApplicationId()))
						.filter(rp -> portfolioTypeId.equals(rp.getPortfolioTypeId()))
						.anyMatch(r -> relatedPortfolioCode.equals(r.getRelatedPortfolioCode())))
				.findFirst();
	}

	@Override
	public String getCurrencyByProductCode(String productCode) {

		final Optional<Product> product = this.getProduct(productCode);
		// if there is Portal Product find the baseCurrency from repo and return
		// its currencyId

		return String.valueOf(product.map(tempProd -> {
			return currencyRepository.getEntity(tempProd.getBaseCurrencyID().toString()).get();
		}).orElse(new Currency().setCurrencyCode("")).getCurrencyCode());

	}

}

com.vanguard.corp.ics.portal.repository
	BasePortalRepository.class
/*
 ****************************************************************************
 *
 * Copyright (c)2017 The Vanguard Group of Investment Companies (VGI)
 * All rights reserved.
 *
 * This source code is CONFIDENTIAL and PROPRIETARY to VGI. Unauthorized
 * distribution, adaptation, or use may be subject to civil and criminal
 * penalties.
 *
 ****************************************************************************
 Module Description:

 $HeadURL: http://prdsvnrepo:8080/svn/tip/tip/via/PSI/Java/SMA/Shared/PortalService/trunk/src/main/java/com/vanguard/corp/ics/portal/repository/BasePortalRepository.java $
 $LastChangedRevision: 603652 $
 $Author: Daghan Acay$
 $LastChangedDate: 2017-10-16 01:56:58 -0400 (Mon, 16 Oct 2017) $
*/
package com.vanguard.corp.ics.portal.repository;

import java.lang.reflect.Array;
import java.lang.reflect.ParameterizedType;
import java.util.Arrays;
import java.util.List;
import java.util.Optional;

import javax.ws.rs.client.Client;
import javax.ws.rs.client.ClientBuilder;
import javax.ws.rs.core.Cookie;
import javax.ws.rs.core.MediaType;

import org.springframework.beans.factory.annotation.Autowired;

import com.vanguard.corp.ics.loggers.Logger;
import com.vanguard.corp.ics.portal.config.PortalSDKConfiguration;
import com.vanguard.corp.ics.portal.exception.PortalException;
import com.vanguard.corp.ics.portal.helper.TokenHelper;
import com.vanguard.jaxrs.feature.SecureClientRequestFeature;

@SuppressWarnings("unchecked")
public abstract class BasePortalRepository<T> {

	private static final Logger LOG = Logger.getLogger(BasePortalRepository.class);

	private static Long WAIT_TIME_MS = 12 * 1000L; // 12 Secs

	@Autowired
	protected PortalSDKConfiguration config;

	/**
	 * Should return URL where the entity can be retrieved and it should always
	 * end with "/" e.g.
	 *
	 * http://satapii.vanguard.com:1080/rs/cf/portal-product-webservice/rs/
	 * products/productcode/
	 *
	 * @return
	 */
	public abstract String getEntityUrl();

	/**
	 * Should return URL where the list of the entities can be retrieved and it
	 * should always end with "/" e.g.
	 *
	 * http://satapii.vanguard.com:1080/rs/cf/portal-product-webservice/rs/
	 * products
	 *
	 * @return
	 */
	public abstract String getListUrl();

	public List<T> getEntities() {
		return Arrays.asList((T[]) getRequest(getListUrl(), getArrayType()));
	}

	public Optional<T> getEntity(String id) {
		return Optional.ofNullable(this.getRequest(getEntityUrl() + id, getType()));
	}

	protected Class<T> getType() {
		return (Class<T>) ((ParameterizedType) getClass().getGenericSuperclass()).getActualTypeArguments()[0];
	}

	protected Class<?> getArrayType() {
		final T[] arrayType = (T[]) Array.newInstance(getType(), 1);
		return arrayType.getClass();
	}

	protected <R> R getRequest(String url, Class<R> returnClass) {
		final Cookie vgSessionCookie = TokenHelper.getVGSessionCookie(config);
		Client client = ClientBuilder.newClient();
		client = client.register(SecureClientRequestFeature.class);

		// Reprocess failures up to retryCount number of times
		int i = 0;
		while (true) {
			try {
				return client.target(url).request(MediaType.APPLICATION_JSON)
						.property(SecureClientRequestFeature.HTTP_AUTHENTICATION_COOKIE, vgSessionCookie)
						.accept(MediaType.APPLICATION_JSON).get(returnClass);

			} catch (final Exception e) {
				LOG.error("Error connecting to portal.");
				if (i < config.getRetryCount()) {
					i++;

					// Wait a bit before retrying
					try {
						Thread.sleep(WAIT_TIME_MS);
					} catch (final InterruptedException e1) {
						LOG.error("Error while waiting to retry. {}", e);
						Thread.currentThread().interrupt();
					}
					LOG.info("Retrying Portal Connection {}/{}", i, config.getRetryCount());
				} else {
					LOG.error("Error making request to: {}, {}", url, e);
					throw new PortalException("Could not connect to Portal");
				}
			}
		}

	}

}

