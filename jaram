	}

	public List<RelatedPortfolio> getRelatedPortfolios() {
		return relatedPortfolios;
	}

	public void setRelatedPortfolios(List<RelatedPortfolio> relatedPortfolios) {
		this.relatedPortfolios = relatedPortfolios;
	}

	private LocalDate parseDate(String dateString) {
		return dateString == null ? null : LocalDate.parse(dateString, DATE_FORMATTER);
	}

	@Override
	public String toString() {
		return "Product [productID=" + productID + ", productCode=" + productCode + "]";
