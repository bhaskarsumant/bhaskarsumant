 Module Description:

 $HeadURL: http://prdsvnrepo:8080/svn/tip/tip/via/PSI/Java/SMA/Shared/PortalService/trunk/src/main/java/com/vanguard/corp/ics/loggers/Logger.java $
 $LastChangedRevision: 601516 $
 $Author: uoex $
 $LastChangedDate: 2017-10-05 20:37:45 -0400 (Thu, 05 Oct 2017) $
*/
package com.vanguard.corp.ics.loggers;

import org.slf4j.helpers.MessageFormatter;

import com.vanguard.services.logging.Log;

public class Logger {

	private static final int DEFAULT_ERROR_CODE = -1;

	private final String className;

	private Logger(String className) {
		this.className = className;
	}

	public static Logger getLogger(Class<?> clazz) {
		return getLogger(clazz.getName());
	}

	public static Logger getLogger(String name) {
		return new Logger(name);
	}

	public void info(String message, Object... args) {
		// Using report instead of info for SMA
		Log.report(className, format(message, args));
	}

	public void trace(String message, Object... args) {
		Log.trace(className, format(message, args));
	}

	public void warn(int code, String message, Object... args) {
		Log.warning(className, format(message, args), code);
	}

	public void error(String message, Object... args) {
		error(DEFAULT_ERROR_CODE, message, args);
	}

	public void error(int code, String message, Object... args) {
		Log.error(className, format(message, args), code);
	}

	private String format(String message, Object... args) {
		return args.length > 0 ? MessageFormatter.arrayFormat(message, args).getMessage() : message;
	}

}

