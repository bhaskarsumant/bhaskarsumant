package com.vanguard.corp.ics.portal.domain;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;

@JsonIgnoreProperties(ignoreUnknown = true)
public class RelatedPortfolio {

	private Long applicationId;

	private Long portfolioTypeId;

	private String relatedPortfolioCode;

	private String realtedPortfolioId;

	public Long getApplicationId() {
		return applicationId;
	}

	public void setApplicationId(Long applicationId) {
		this.applicationId = applicationId;
	}

	public Long getPortfolioTypeId() {
		return portfolioTypeId;
	}

	public void setPortfolioTypeId(Long portfolioTypeId) {
		this.portfolioTypeId = portfolioTypeId;
	}

	public String getRelatedPortfolioCode() {
		return relatedPortfolioCode;
	}

	public void setRelatedPortfolioCode(String relatedPortfolioCode) {
		this.relatedPortfolioCode = relatedPortfolioCode;
	}

	public String getRealtedPortfolioId() {
		return realtedPortfolioId;
	}

	public void setRealtedPortfolioId(String realtedPortfolioId) {
		this.realtedPortfolioId = realtedPortfolioId;
	}

	@Override
	public String toString() {
		return "RelatedPortfolio [applicationId=" + applicationId + ", portfolioTypeId=" + portfolioTypeId
				+ ", relatedPortfolioCode=" + relatedPortfolioCode + ", realtedPortfolioId=" + realtedPortfolioId + "]";
	}
}

