CalculationConfigurationDAO.class
package com.vanguard.corp.ics.dao.target;

import java.io.Serializable;
import java.util.List;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.EnumType;
import javax.persistence.Enumerated;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.JoinTable;
import javax.persistence.ManyToMany;
import javax.persistence.OneToMany;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

import com.vanguard.corp.ics.enums.CalculationConfigEnum;

/**
 * The persistent class for the TCALC_CONFIG database table.
 *
 */
@Entity
@Table(name = "SDLTADM.TCALC_CONFIG")
public class CalculationConfigurationDAO implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@SequenceGenerator(name = "TCALC_CONFIG_CALCCONFIGID_GENERATOR", sequenceName = "SDLTADM.SEQ1CALCONFIGID")
	@GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "TCALC_CONFIG_CALCCONFIGID_GENERATOR")
	@Column(name = "CALC_CONFIG_ID")
	private long calcConfigId;

	@Enumerated(EnumType.STRING)
	@Column(name = "CALC_CONFIG")
	private CalculationConfigEnum calcConfig;

	// bi-directional many-to-many association to TdataSrc
	@ManyToMany
	@JoinTable(name = "SDLTADM.TCCONF_DS_JOIN", joinColumns = {
			@JoinColumn(name = "CALC_CONFIG_ID_FK")
	}, inverseJoinColumns = {
			@JoinColumn(name = "DATA_SRC_ID_FK")
	})
	private List<DatasourceDAO> tdataSrcs;

	// bi-directional many-to-one association to TcalcRun
	@OneToMany(mappedBy = "tcalcConfig")
	private List<CalculationRunDAO> tcalcRuns;

	public CalculationConfigurationDAO() {
	}

	public long getCalcConfigId() {
		return this.calcConfigId;
	}

	public void setCalcConfigId(long calcConfigId) {
		this.calcConfigId = calcConfigId;
	}

	public CalculationConfigEnum getCalcConfig() {
		return this.calcConfig;
	}

	public void setCalcConfig(CalculationConfigEnum calcConfig) {
		this.calcConfig = calcConfig;
	}

	public List<DatasourceDAO> getTdataSrcs() {
		return this.tdataSrcs;
	}

	public void setTdataSrcs(List<DatasourceDAO> tdataSrcs) {
		this.tdataSrcs = tdataSrcs;
	}

	public List<CalculationRunDAO> getTcalcRuns() {
		return this.tcalcRuns;
	}

	public void setTcalcRuns(List<CalculationRunDAO> tcalcRuns) {
		this.tcalcRuns = tcalcRuns;
	}

	public CalculationRunDAO addTcalcRun(CalculationRunDAO tcalcRun) {
		getTcalcRuns().add(tcalcRun);
		tcalcRun.setTcalcConfig(this);

		return tcalcRun;
	}

	public CalculationRunDAO removeTcalcRun(CalculationRunDAO tcalcRun) {
		getTcalcRuns().remove(tcalcRun);
		tcalcRun.setTcalcConfig(null);

		return tcalcRun;
	}

}

				
